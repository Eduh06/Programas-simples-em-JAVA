import java.util.Scanner;

public class main {

	public static void main(String[] args) {
		Scanner scan = new Scanner (System.in);   //Faz a leitura do teclado
		
		System.out.println("Tabuada: ");          //solicita qual tabuada o usuario quer 
		int tabuada = scan.nextInt();
		
		System.out.println("Tabuada de " + tabuada);  
		
		for(int i = 1; i<=10; i = i + 1) {
			System.out.println(tabuada + " X " + i + " = " + (tabuada*i));        // faz o loop efetuando calculo  e apresentado resultado na tela
			
		}

	}

}
